@model OlineAuctionMarketing.Models.DTO.Auctioneer.AuctioneersResponseModel

<div>
    <h4>Auctioneer</h4>
    <hr />

    <table class="table">
        <thead>
            <tr>
                <th>
                    First name
                </th>
                <th>
                    Last Name
                </th>

                <th>
                    Email
                </th>
                <th>
                    Phone number
                </th>
                <th>
                    Actions
                </th>
            </tr>
        </thead>
        <tbody>

            @foreach (var item in Model.Data)
            {
                <tr>
                    <td scope="row">
                        @item.FirstName

                    </td>

                    <td scope="row">
                        @item.LastName

                    </td>
                    <td scope="row">
                        @item.Email

                    </td>
                    <td scope="row">
                        @item.PhoneNumber

                    </td>
                    <td scope="row">
                        <a asp-action="Details" asp-route-id="@item.Id" class="btn btn-primary">View</a>
                        <a asp-action="Edit" asp-route-id="@item.Id" class="btn btn-primary"><i class="fas fa-edit"></i></a>
                        <a asp-action="Delete" asp-route-id="@item.Id" class="btn btn-danger"><i class="fa-sharp fa-solid fa-trash"></i> </a>

                    </td>

                </tr>
            }
        </tbody>
    </table>
    <form asp-action="Create">
    </form>
