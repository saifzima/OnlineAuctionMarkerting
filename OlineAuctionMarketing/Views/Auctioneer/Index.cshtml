@model OlineAuctionMarketing.Models.DTO.Auctioneer.AuctioneersResponseModel
<div>
    <h4>Auctioneer</h4>
    <hr />

    <table class="table">
        <thead>
            <tr>
                <th>
                    First name
                </th>
                <th>
                    Last Name
                </th>

                <th>
                    Password
                </th>

                <th>
                    Email
                </th>

                <th>
                    Phone number
                </th>
                <th>
                    Actions
                </th>

            </tr>
        </thead>
        <tbody>

            @foreach (var item in Model.Data)
            {
                <tr>
                    <td scope="row">
                        @item.FirstName

                    </td>

                    <td scope="row">
                        @item.LastName

                    </td>
                    <td scope="row">
                        @item.Email

                    </td>
                    <td scope="row">
                        @item.Password

                    </td>
                    <td scope="row">
                        @item.PhoneNumber

                    </td>
                    <td scope="row">
                        <a asp-action="" asp-route-id="" class="btn btn-primary">View</a>
                        <a asp-action="Edit" asp-route-id="@item.Id" class="btn btn-primary">Edit</a>
                        <a asp-action="" asp-route-id="" class="btn btn-danger">Delete</a>
                    </td>

                </tr>
            }
        </tbody>
    </table>
